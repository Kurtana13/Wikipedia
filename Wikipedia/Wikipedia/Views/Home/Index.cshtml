@model IEnumerable<User>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                User Id
            </th>
            <th>
                Name
            </th>
        </tr>
    </thead>
    <tbody>
        @if(Model.Count() != 0)
        {
            @foreach (User user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Name</td>
                </tr>
            }
        }
       
    </tbody>
</table>